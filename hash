using System;
using System.Text;
using DataStructures;

namespace HashTable
{
    class HashTable  // hash class
    {
        
        private int count;
        private int Size;

        public HashTable(int size)
        {
            count = 0;
            Size = size;
            
        }

        public void Add(string item)
        {
            HashTable.Add(item);
            count++;
        }

        public void Remove(string item)
        {
            HashTable.Remove(item);
            count--;
        }

    

    }
}
